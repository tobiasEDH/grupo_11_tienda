<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head.ejs') %>
    <link href="/css/listado-usuarios.css" rel="stylesheet">
    <title>Listado de productos</title>
</head>
<body>
    <%- include('partials/header.ejs') %>
    <main>
        <% users.forEach(user=>{ %>
        <a id="link-usuario" href="/usuario/<%= user.ID_Usuario %>">
            <article class="productos">
            <img src="/img/users/<%= user.Imagen%>" alt="">
            <h3><strong>Nombre: </strong><%= user.Nombre%></h3>
            <h3><strong>Apellido: </strong><%= user.Apellido%></h3>
            <h3><strong>Email: </strong><%= user.Email%></h3>
        </article>
        </a>
        <% }) %>
    </main>
    <%- include('partials/footer.ejs') %>
</body>
</html>